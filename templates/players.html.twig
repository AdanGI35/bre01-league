{% extends "layout.html.twig" %}
{% block content %}
{% block players %}
{% for player in players %}
                    <section class="player_cards">
                        <img src="{{ player.team.logo.url}}" alt="{{ player.team.logo.alt}}">
                        <p>{{ player.getNickName() }}</p>
                        
                    </section>
            {% endfor %}
{% endblock %}
{% endblock %}